08. Create Table Users

create table Users
(
	Id bigint primary key identity,
	Username varchar(30) not null,
	[Password] varchar(26) not null,
	ProfilePicture varchar(max),
	LastLoginTime datetime,
	IsDeleted bit
		
)

insert into Users 
(Username, [Password], ProfilePicture, LastLoginTime, IsDeleted)
values 
('adad', 'strondsadsagpass123', 'https://www.youtube.com/?gl=BG&hl=bg', '1/5/2021', 0),
('stasdasdasdsoshop', 'strongpass123', 'https://www.youtube.com/?gl=BG&hl=bg', '3/15/2021', 0),
('stosgsdop', 'stronggsdgsdpass123', 'https://www.youtube.com/?gl=BG&hl=bg', '3/12/2021', 0),
