13. Movies Database

create table Directors
(
	Id int primary key identity,
	DirectorName varchar(25) not null,
	Notes varchar(max)
)

insert Directors (DirectorName, Notes)
values
('Stanislav', 'nsh'),
('dasda', 'nsh'),
('Stanigsdgaslav', null),
('Stanisdasdlav', null),
('Stanadadislav', 'nsddh')

create table Genres
(
    Id int primary key identity,
	GenreName varchar(25) not null,
	Notes varchar(max)
)
insert Genres(GenreName, Notes)
values
('Stanislav', 'nsh'),
('dasda', 'nsh'),
('Stanigsdgaslav', null),
('Stanisdasdlav', null),
('Stanadadislav', 'nsddh')

create table Categories
(
    Id int primary key identity,
	CategoryName varchar(25) not null,
	Notes varchar(max)
)
insert Categories(CategoryName, Notes)
values
('Stanislav', 'nsh'),
('dasda', 'nsh'),
('Stanigsdgaslav', null),
('Stanisdasdlav', null),
('Stanadadislav', 'nsddh')

create table Movies
(
	Id int primary key identity,
	Title varchar(25) not null,
	DirectorId int not null,
	CopyrightYear varchar(25),
	[Length] int,
	GenreId int not null,
	CategoryId int not null,
	Rating int,
	Notes varchar(max)
)

insert Movies(Title, DirectorId, CopyrightYear, [Length], GenreId, CategoryId, Rating, Notes)
values
('asdads', 3, 'dasdsa', 5, 1, 2 ,3, 'nsasdh'),
('asdads', 33, 'dasasddsa', 5, 1, 2 ,3, 'nsasdh'),
('asdads', 333, 'dasdsa', 325, 1, 2 ,3, 'nshad'),
('asdads', 3231, 'dasdsasda', 5, 1, 2 ,3, 'nshdsa'),
('asdads', 3123, 'dasdsada', 5, 1, 2 ,3, 'nsadsh')


